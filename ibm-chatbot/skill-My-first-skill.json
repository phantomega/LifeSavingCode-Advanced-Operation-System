    {
      "type": "response_condition",
      "output": {
        "generic": [
          {
            "values": [
              {
                "text": "1. Let victim sit up straight."
              },
              {
                "text": "2. Encourage victim to breathe deeply."
              },
              {
                "text": "3. If he has an inhaler, ask him use it. Assist if needed."
              },
              {
                "text": "4. Monitor the victim after using the inhaler."
              },
              {
                "text": "5. If no relief after few min, call ambulance."
              }
            ],
            "response_type": "text",
            "selection_policy": "multiline"
          }
        ]
      },
      "parent": "node_10_1592122148731",
      "conditions": "@non-fire-incident:Asthma",
      "dialog_node": "response_3_1592122218080",
