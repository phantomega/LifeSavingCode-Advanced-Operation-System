    },
    {
      "type": "response_condition",
      "output": {
        "generic": [
          {
            "values": [
              {
                "text": "1. Call ambulance immediately."
              },
              {
                "text": "2. Prevent the victim from walking or standing."
              },
              {
                "text": "3. Treat injuries, if any."
              },
              {
                "text": "4. If the victim does not respond and stops breathing, start CPR."
              }
            ],
            "response_type": "text",
            "selection_policy": "multiline"
          }
        ]
      },
      "parent": "node_10_1592122148731",
